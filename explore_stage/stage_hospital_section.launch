<!--
  Do explore & slam in hospital-section.world
-->

<launch>
  <master auto="start"/>
  <param name="/use_sim_time" value="true"/>

  <node pkg="stage" type="stageros" name="stage" args="$(find bosch_worlds)/hospital-section.world" respawn="false" output="screen"/>

<!--
  <include file="$(find explore_stage)/gmapping.xml" />
-->
  <node pkg="map_server" type="map_server" name="map_server" args="$(find bosch_maps)/hospital.yaml">
    <param name="frame_id" value="map" />
  </node>
  <node pkg="tf" type="static_transform_publisher" name="map_odom" args="105 40 0 0 0 0 map odom 100" />

  <!-- These being world specific may be unnecessary... -->
  <include file="$(find explore_stage)/world_configs/stage_hospital_section/move.xml" />
  <include file="$(find explore_stage)/world_configs/stage_hospital_section/explore_slam.xml" />
</launch>
